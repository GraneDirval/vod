{
  "type": "project",
  "license": "proprietary",
  "repositories":[
    { "type": "git", "url": "https://git-codecommit.eu-west-1.amazonaws.com/v1/repos/store-difftool-module" }
  ],
  "require": {
    "php": "^7.2",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "^1.6",
    "ext-redis": "*",
    "aws/aws-sdk-php": "^3.84",
    "beberlei/DoctrineExtensions": "^1.2",
    "cloudinary/cloudinary_php": "1.13.0",
    "e-moe/guzzle6-bundle": "^1.2",
    "enqueue/enqueue-bundle": "0.9.3",
    "geoip2/geoip2": "v2.9.0",
    "guzzlehttp/guzzle": "v6.3.3",
    "league/flysystem-aws-s3-v3": "dev-master",
    "misd/phone-number-bundle": "^1.3",
    "mobiledetect/mobiledetectlib": "^2.8",
    "nesbot/carbon": "2.10.0",
    "phive/twig-extensions-deferred": "2.0",
    "piwik/piwik-php-tracker": "1.0",
    "playwing/difftool": "dev-master",
    "queue-interop/queue-interop": "0.8.0",
    "ramsey/uuid": "3.8.0",
    "sensio/framework-extra-bundle": "v5.2.4",
    "sonata-project/admin-bundle": "3.43.0",
    "sonata-project/doctrine-orm-admin-bundle": "3.7.0",
    "symfony/asset": "v4.2.1",
    "symfony/console": "v4.2.1",
    "symfony/event-dispatcher": "^4.2",
    "symfony/expression-language": "v4.2.1",
    "symfony/flex": "v1.1.8",
    "symfony/form": "v4.2.1",
    "symfony/framework-bundle": "v4.2.1",
    "symfony/monolog-bundle": "v3.3.1",
    "symfony/orm-pack": "v1.0.5",
    "symfony/swiftmailer-bundle": "v3.2.4",
    "symfony/twig-bridge": "v4.2.1",
    "symfony/twig-bundle": "v4.2.1",
    "symfony/validator": "v4.2.1",
    "symfony/web-link": "v4.2.1",
    "symfony/yaml": "v4.2.1"
  },


  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "3.1.0",
    "liip/functional-test-bundle": "v2.x-dev",
    "mockery/mockery": "1.2.0",
    "phpunit/phpunit": "7",
    "symfony/debug-pack": "v1.0.7",
    "symfony/maker-bundle": "v1.11.1",
    "symfony/profiler-pack": "v1.0.4",
    "symfony/test-pack": "v1.0.2",
    "symfony/web-server-bundle": "v3.4.20"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "": "src/",
      "App\\": "src/App",
      "Admin\\": "src/Admin"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false
    }
  }
}
