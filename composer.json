{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.2",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "cloudinary/cloudinary_php": "1.13.0",
    "geoip2/geoip2": "v2.9.0",
    "ramsey/uuid": "3.8.0",
    "sensio/framework-extra-bundle": "v5.2.4",
    "sonata-project/admin-bundle": "3.43.0",
    "sonata-project/doctrine-orm-admin-bundle": "3.7.0",
    "symfony/asset": "v3.4.20",
    "symfony/console": "v3.4.20",
    "symfony/expression-language": "v3.4.20",
    "symfony/flex": "v1.1.8",
    "symfony/form": "v3.4.20",
    "symfony/framework-bundle": "v3.4.20",
    "symfony/monolog-bundle": "v3.3.1",
    "symfony/orm-pack": "v1.0.5",
    "symfony/swiftmailer-bundle": "v3.2.4",
    "symfony/twig-bundle": "v3.4.20",
    "symfony/validator": "v3.4.20",
    "symfony/web-link": "v3.4.20",
    "symfony/yaml": "v3.4.20"
  },


  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "3.1.0",
    "symfony/debug-pack": "v1.0.7",
    "symfony/maker-bundle": "v1.11.1",
    "symfony/profiler-pack": "v1.0.4",
    "symfony/test-pack": "v1.0.2",
    "symfony/web-server-bundle": "v3.4.20"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "": "src/",
      "App\\": "src/App",
      "Admin\\": "src/Admin"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false
    }
  }
}
