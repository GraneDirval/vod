{% macro render() %}
    {% if hasActiveSubscription() %}
        {{ translate('subscription.status.subscribed')|raw|nl2br }}
    {% elseif isUnsubscribed() %}
        {{ translate('subscription.status.unsubscribed')|raw|nl2br }}
    {% elseif hasInActiveSubscription() %}
        hasInActiveSubscription (need text)
    {% elseif hasSubscriptionWithError() %}
        {{ translate('subscription.status.error') }}
    {% elseif isNotEnoughCredit() %}
        {{ translate('subscription.status.not_enough_credit') }}
    {% elseif not isSubscriptionExist() %}
        {{ translate('subscription.status.not_subscribe')|raw|nl2br }}
    {% endif %}
{% endmacro render %}